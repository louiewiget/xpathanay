<script>
 
 var thisPage = '1';
  var pageCount = '5';
  function gotoPage() {
    var pageNum = $("#pageNum").val();
    if (pageNum == '') {
      alert('请输入页号！');
      return;
    }
    else if (parseInt(pageNum)>parseInt(pageCount)) {
      return;
    }
    else {
      var pag = parseInt(pageNum);
      if (isNaN(pag)) {
        alert('页号必须是一个整数！');
        return;
      }
      else {
        if (pag.toString().length != pageNum.length) {
          alert('页号必须是一个整数！');
          return;
        }
      }
    }
    location.href = 'LabelAction!getNewsByTypeGoTo.do?pageNum=' + pageNum;
  }
  function first() {
    location.href = "LabelAction!getNewsByTypeGoTo.do?pageNum=1";
  }

  function previousPage() {
    location.href = "LabelAction!getNewsByTypeGoTo.do?pageNum=" + (parseInt(thisPage) - 1);
  }

  function nextPage() {
    location.href = "LabelAction!getNewsByTypeGoTo.do?pageNum=" + (parseInt(thisPage) + 1);
  }

  function last() {
    location.href = "LabelAction!getNewsByTypeGoTo.do?pageNum=" + pageCount;
  }
  
	function calcPageHeight(doc) {
     var cHeight = Math.max(doc.body.clientHeight, doc.documentElement.clientHeight)
     var sHeight = Math.max(doc.body.scrollHeight, doc.documentElement.scrollHeight)
     var height  = Math.max(cHeight, sHeight)
     return height
	}
$(function(){
 var height = calcPageHeight(document);
 if(height<=150){
 	parent.document.getElementById('newsList').style.height = (height-20)+ 'px' 
 }else{
 	parent.document.getElementById('newsList').style.height = height + 'px' 
 }
})
  	 
	
</script>
